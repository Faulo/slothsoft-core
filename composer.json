{
	"name" : "slothsoft/core",
	"description" : "Core stuff.",
	"license" : "WTFPL",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"guzzlehttp/psr7" : "^1.4",
		"php" : "^7.2",
		"psr/http-message" : "^1.0",
		"slothsoft/w3c" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6.5"
	},
	"suggests" : {
		"ext-curl" : "*"
