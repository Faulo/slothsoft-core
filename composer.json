{
	"name" : "slothsoft/core",
	"description" : "Core stuff.",
	"license" : "WTFPL",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"ext-curl" : "*",
		"php" : ">=7.0",
		"slothsoft/w3c" : "dev-master",
		"slothsoft/blob" : "dev-master",
		"psr/http-message" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : ">=6.5"
	},
	"suggests" : {
		"ext-pthreads" : ">=2.0"
	},
	"autoload" : {
		"psr-0" : {
			"Slothsoft\\Core\\" : "src/"
		},
		"files" : [
			"src/autoload.php"
		]
	},
	"keywords" : [
		"farah-abstract",
		"slothsoft"
