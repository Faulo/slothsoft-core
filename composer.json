{
	"name" : "slothsoft/core",
	"description" : "Core stuff.",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.2",
		"ext-xsl" : "*",
		"php-ds/php-ds" : "^1.2",
		"slothsoft/w3c" : "^1.1",
		"psr/http-message" : "^1.0",
		"guzzlehttp/psr7" : "^1.4",
		"symfony/process" : "^5.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"suggest" : {
		"ext-curl" : "*",
		"ext-com_dotnet" : "*"
	},
	"autoload" : {
		"files" : [
			"scripts/bootstrap.php"
		],
		"psr-4" : {
			"Slothsoft\\Core\\" : "src"
		}
	},
	"keywords" : [
		"slothsoft",
		"docs"
	],
	"bin" : [
		"scripts/server-clean"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Core\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.11.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/core"
}